{
    "command": "/usr/sbin/apache2 -DFOREGROUND",
    "config_files": [
        {
            "source": "/data/keystone.conf",
            "dest": "/etc/keystone/keystone.conf",
            "owner": "keystone",
            "perm": "0600"
        },
        {
            "source": "/data/0",
            "dest": "/etc/keystone/fernet-keys/0",
            "owner": "keystone",
            "perm": "0600"
        },
        {
            "source": "/data/1",
            "dest": "/etc/keystone/fernet-keys/1",
            "owner": "keystone",
            "perm": "0600"
        }
    ]
}
